@model IEnumerable<LanchesMac.Models.Lanche>

<div>
    <h2>Todos os lanches</h2>
</div>


<div class="container">
<div class="row">
@foreach (var lanche in Model)
{

    <div class="col-sm-4 col-md-4 col-lg-4"> 
    <p><img scr="@lanche.ImagemUrl" class="img-fluid" /></p>
        <h4>@lanche.Nome</h4>
        <p><img src="@lanche.ImagemUrl"></p>
        <h3>Preço: @lanche.Preco.ToString("c")</h3>
    </div>
}

    </div>
    </div>
   
   
   